		meta(name="author", content="Alejandro H Tadino M")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		link(rel='stylesheet' href='css/styles.css')
		script(src="https://use.fontawesome.com/8940ad064b.js")
	body
		
