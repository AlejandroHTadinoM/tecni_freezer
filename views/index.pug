						li.nav-item.twelve.columns
							a(href="#contact")
									|CONTACTOS


		nav.navbar
			.container
				.row
					.twelve.columns
						ul
							li.nav-item
								a(href="#home")
									|INICIO
							li.nav-item
								a(href="#service")
									|SERVICIOS
							li.nav-item
