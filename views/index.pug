			i(class="fa fa-bars" aria-hidden="true")

		.menu-open
			.container
				.row
					ul
						li.twelve.columns
							a
								i(class="fa fa-times" aria-hidden="true").menu-icon-close
						li.nav-item.twelve.columns
							a(href="#home")
								|INICIO
						li.nav-item.twelve.columns
							a(href="#service")
